{
  "manifest_version": 2,

  "name": "MAdKiller",
  "version": "0.1",
  "description": "Send Page to a server and test for malvertising",

  "browser_action": {
    "default_icon": "icon19.png"
  },

  "background": {
    "scripts": ["event.js"],
    "persistent": false
  },
  
  "permissions": [
      "<all_urls>",
      "tabs"
  ]
}